{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aaronfrancis/pulse-outdated": "^0.1.2",
        "barryvdh/laravel-snappy": "^1.0",
        "bezhansalleh/filament-shield": "^3.2",
        "coolsam/modules": "^4.0",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "kalnoy/nestedset": "^6.0",
        "knplabs/knp-snappy": "^1.5",
        "laravel/framework": "^11.9",
        "laravel/horizon": "^5.24",
        "laravel/pail": "^1.1",
        "laravel/pulse": "^1.2",
        "laravel/reverb": "@beta",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.9",
        "leventcz/laravel-top": "^1.1",
        "predis/predis": "^2.0",
        "spatie/browsershot": "^4.1",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-medialibrary": "^11.7",
        "spatie/laravel-pdf": "^1.5",
        "spatie/laravel-permission": "^6.7",
        "timacdonald/pulse-validation-errors": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "barryvdh/laravel-ide-helper": "^3.0",
        "enlightn/enlightn": "^2.10",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^2.0",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "nunomaduro/phpinsights": "^2.11",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Helpers/load-helpers.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "aliases": {
            "DepartmentLookup": "App\\Facades\\DepartmentLookup",
            "BooleanEvaluator": "App\\Facades\\BooleanEvaluator",
            "UserLookup": "App\\Facades\\UserLookup",
            "ApiClient": "App\\Facades\\ApiClient"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
